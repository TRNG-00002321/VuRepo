							"\r",
							"pm.environment.set(\"uniqueEmail\", uniqueEmail);\r",
							"console.log(\"Generated email:\", uniqueEmail);\r",
							"\r",
							"// Function to generate random number in range\r",
							"function randomBetween(min, max) {\r",
							"    return Math.floor(Math.random() * (max - min + 1)) + min;\r",
							"}\r",
							"\r",
							"// Generate test data\r",
							"pm.environment.set(\"randomAge\", randomBetween(18, 65));\r",
							"pm.environment.set(\"randomQuantity\", randomBetween(1, 10));\r",
							"pm.environment.set(\"randomPrice\", (randomBetween(999, 9999) / 100).toFixed(2));\r",
